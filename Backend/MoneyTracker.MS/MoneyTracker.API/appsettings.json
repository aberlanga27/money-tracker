{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning"
        },
        "Seq": {
            "BaseAddress": "#{SEQ_BASE_ADDRESS}",
            "ApiKey": "#{SEQ_API_KEY}"
        }
    },
    "AllowedHosts": "*",
    "AllowedOrigins": [
        "http://localhost:5173",
        "https://localhost:5173"
    ],
    "ConnectionStrings": {
        "DefaultConnection": "#{DEFAULT_CONNSTR}"
    },
    "Encryptor": {
        "Key": "#{ENCRYPTOR_KEY}",
        "IV": "#{ENCRYPTOR_IV}",
        "CertificatePassword": "#{ENCRYPTOR_CERTIFICATE_PASSWORD}"
    },
    "Jwt": {
        "Key": "#{JWT_KEY}",
        "Issuer": "admin.mmmYYYY.com",
        "Audience": "appname.mmmYYYY.com",
        "ExpirationInMinutes": 180
    },
    "Workers": {
        "SampleInterval": 60
    },
    "Smtp": {
        "Server": "",
        "Port": 0,
        "Username": "",
        "Password": ""
    },
    "Localization": {
        "Default": "en-US",
        "Supported": [
            "en-US",
            "es-MX"
        ]
    },
    "Pagination": {
        "DefaultPageSize": 10,
        "MaxPageSize": 100
    },
    "Cache": {
        "Instance": "#{REDIS_ENDPOINT}",
        "ExpirationInMinutes": 60
    }
}